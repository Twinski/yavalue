{
  "name": "@yapp/yavalue",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "version": "0.0.0",
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "module": "dist/esm/index.es5.js",
  "es2015": "dist/es2015/index.js",
  "license": "MIT",
  "scripts": {
    "setup": "./node_modules/.bin/npm-install-peers && git branch -u origin/master",
    "build": "rimraf dist && npm run build:es2015 && npm run build:esm && npm run build:cjs",
    "build:es2015": "tsc --module es2015 --target es2015 --outDir dist/es2015",
    "build:esm": "tsc --module es2015 --target es5 --outDir dist/esm",
    "build:cjs": "tsc --module commonjs --target es5 --outDir dist/cjs",
    "prepare": "yarn build",
    "push": "git add -A && git commit -am 'quick push' && git push -u origin master",
    "pub": "yarn np",
    "test": "yarn jest"
  },
  "devDependencies": {
    "@types/jest": "^26.0.13",
    "@types/react": "^16.9.49",
    "jest": "^26.4.2",
    "np": "^6.5.0",
    "npm-install-peers": "^1.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.3.0",
    "tsc": "^1.20150623.0",
    "typescript": "^4.0.2"
  },
  "np": {
    "yarn": false,
    "cleanup": false
  },
  "dependencies": {
    "is-plain-object": "^5.0.0"
  }
}
